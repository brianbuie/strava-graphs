{
  "name": "strava-graphs",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev-client": "gatsby develop",
    "dev-server": "nodemon --watch server.js -x node -r dotenv/config -r esm server.js",
    "dev": "concurrently \"yarn dev-client\" \"yarn dev-server\"",
    "build": "gatsby build",
    "start": "NODE_ENV=production node -r esm server.js",
    "stage": "ENV_FILE=true yarn build && NODE_ENV=production node -r dotenv/config -r esm server.js"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "gatsby": "^2.13.57",
    "gatsby-plugin-env-variables": "^1.0.1",
    "react": "^16.9.0",
    "react-dom": "^16.9.0"
  }
}
