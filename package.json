{
  "name": "strava-graphs",
  "version": "1.0.0",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev-client": "PORT=3000 react-scripts start",
    "dev-server": "nodemon --ignore src/ -x node -r dotenv/config -r esm server.js",
    "dev": "concurrently \"yarn dev-client\" \"yarn dev-server\"",
    "build": "react-scripts build",
    "start": "NODE_ENV=production node -r esm server.js",
    "stage": "yarn build && NODE_ENV=production node -r dotenv/config -r esm server.js"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "memory-cache": "^0.2.0",
    "node-fetch": "^2.6.0",
    "query-string": "^6.8.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.1.0",
    "styled-components": "^4.3.2"
  },
  "devDependencies": {
    "concurrently": "^4.1.1",
    "nodemon": "^1.19.1"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
